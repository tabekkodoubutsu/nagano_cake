 <div class="row">      
<div class="container">
<div class="row">
    <!--ここからサイドバー-->
<table class='table table-hover table-inverse col-md-2 '>
   <thead><tr>
   <th>ジャンル検索</th>
  </tr></thead>
   <% @genres.each do |genre| %>
   <tr><td><%= genre.name %></td></tr>
   <% end %>
   </table>
   <!--ここまでサイドバー-->
  <!--会員の商品一覧（public item index)-->
   <div class="col-md-9">
   <h2 class="text-center">商品一覧<small>(全 <%= @items.count %>件)</small></h2>
    
          <div class="row">
          <% @items.each do |item| %>
          <div class="col-md-3">
          <%= attachment_image_tag item,:image,:fill,500,350, fallback: "no-image.jpg",size:'150x100' %><br>
          <%= link_to item.name,item_path(item) %><br>
          ¥<%= item.price %>
          </div>
          <% end %>
          </div></div></div></div></div>
 
    